{
  "name": "reactive-observables",
  "version": "2.0.0-beta-1",
  "description": "Framework-agnostic observables and computed properties similar to Knockout and Vue, built on RxJS",
  "keywords": [
    "observable",
    "observer",
    "subscription",
    "subscriber",
    "reactive",
    "functional",
    "rxjs",
    "knockout",
    "knockout.js",
    "vue.js",
    "vue",
    "computed",
    "observablearray",
    "decorators"
  ],
  "main": "./lib/main.js",
  "types": "./lib/main.d.ts",
  "scripts": {
    "build": "./node_modules/.bin/gulp build",
    "test": "./node_modules/.bin/jasmine-ts \"spec/**/*.spec.ts\"",
    "coverage": "./node_modules/.bin/nyc npm test",
    "report": "./node_modules/.bin/nyc report -r json -x \"*.spec.ts\"",
    "merge": "nyc merge .nyc_output coverage/coverage.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephenjjbrown/reactive-observables.git"
  },
  "author": "Stephen Brown",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stephenjjbrown/reactive-observables/issues"
  },
  "homepage": "https://github.com/stephenjjbrown/reactive-observables#readme",
  "dependencies": {
    "rxjs": "^6.6.7",
    "shallow-equal": "^1.2.1"
  },
  "devDependencies": {
    "@types/jasmine": "^3.8.2",
    "@types/node": "^16.4.12",
    "chai": "^4.3.4",
    "chai-spies": "^1.0.0",
    "eslint": "^7.32.0",
    "gulp": "^4.0.0",
    "gulp-better-rollup": "^3.4.0",
    "gulp-typescript": "^5.0.1",
    "gulp-util": "^3.0.8",
    "istanbul": "^0.4.5",
    "jasmine": "^3.8.0",
    "jasmine-node": "^3.0.0",
    "jasmine-ts": "^0.4.0",
    "nyc": "^15.1.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^3.4.0",
    "ts-node": "^10.1.0",
    "typescript": "^3.9.10"
  }
